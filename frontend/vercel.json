{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_BACKEND_URL": "@backend_url",
    "VITE_GEMINI_API_KEY": "@gemini_api_key",
    "VITE_LIVEKIT_URL": "@livekit_url",
    "VITE_LIVEKIT_API_KEY": "@livekit_api_key",
    "VITE_LIVEKIT_API_SECRET": "@livekit_api_secret"
  }
}
